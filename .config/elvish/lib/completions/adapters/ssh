source $1/bash-completion/bash_completion && source $1/bash-completion/completions/ssh
shift
COMP_CWORD=$1
shift
COMPREPLY=()
COMP_WORDBREAKS='"'"'"'><=;|&(:'
COMP_LINE="$@"
COMP_WORDS=($COMP_LINE)
COMP_POINT=${#COMP_LINE}
_ssh 2>/dev/null # elvish is looking for StdErr also
printf '%s\n' "${COMPREPLY[@]}"
