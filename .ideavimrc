" https://github.com/JetBrains/ideavim
" https://github.com/JetBrains/ideavim/blob/master/src/com/maddyhome/idea/vim/package-info.java
" https://gist.github.com/zchee/9c78f91cc5ad771c1f5d

set clipboard^=unnamed,unnamedplus

let mapleader = " "

set visualbell
set noerrorbells
set multiple-cursors
set surround
set number
set incsearch hlsearch
set ignorecase smartcase
set scrolloff=5
set relativenumber
set showmode

set timeoutlen=1000

inoremap jk <Esc>
vnoremap jk <Esc>


" navigate back/forwar
nnoremap ;t :action JumpToLastWindow<cr>
" nnoremap <leader>z :action HideAllWindows<cr>
    
nnoremap <Space>sv :source ~/.ideavimrc<CR>
nnoremap <leader>G :action ToggleDistractionFreeMode<cr>
nnoremap <leader><leader>g :action HideAllWindows<cr>
nnoremap <leader>cv :action ChangeView<cr>

nnoremap <leader>ff :action FindInPath<cr>
nnoremap <leader>fu :action FindUsages<cr>
nnoremap <leader>ga :action GotoAction<cr>
nnoremap <leader>gb :action Annotate<cr>
" nnoremap gd :action GotoDeclaration<cr>
nnoremap <leader>gi :action GotoImplementation<cr>
" nnoremap <leader>gq :action ReformatCode<cr>
nnoremap <leader>gr :action GotoRelated<cr>
nnoremap <leader>gs :action GotoSuperMethod<cr>
" nnoremap <leader>gt :action GotoTest<cr>
nnoremap <leader>gt :action GotoTypeDeclaration<cr>
nnoremap <leader>ic :action InspectCode<cr>
nnoremap <leader>mv :action ActivateMavenProjectsToolWindow<cr>
nnoremap <leader>oi :action OptimizeImports<cr>
nnoremap <leader>q  :action CloseActiveTab<cr>
nnoremap <leader>rc :action ChooseRunConfiguration<cr>
nnoremap <leader>re :action RenameElement<cr>
nnoremap <leader>rf :action RenameFile<cr>
nnoremap <leader>rr :action Run<cr>
" nnoremap <leader>tl Vy<cr>:action ActivateTerminalToolWindow<cr>
nnoremap <leader>zi :action EditorIncreaseFontSize<cr>
nnoremap <leader>zo :action EditorDecreaseFontSize<cr>
nnoremap [c :action VcsShowPrevChangeMarker<cr>
nnoremap ]c :action VcsShowNextChangeMarker<cr>

nnoremap <leader>de :action ShowErrorDescription<cr>
nnoremap gc :action CommentByLineComment<cr>
vnoremap <leader>tl y<cr>:action ActivateTerminalToolWindow<cr>
vnoremap gc :action CommentByLineComment<cr>


" fixed, synced
" default
nnoremap J gJ
nnoremap Q @q
nnoremap Y y$
nnoremap C c$

" toggle
nnoremap [e :action GotoPreviousError<cr> :action ShowErrorDescription<cr>
nnoremap ]e :action GotoNextError<cr> :action ShowErrorDescription<cr>
nnoremap ;s :source ~/.ideavimrc<cr>
nnoremap ;z :action HideAllWindows<cr>
" nnoremap ;f :action ActivateProjectToolWindow<cr>
nnoremap ;f :action SelectInProjectView<cr>
nnoremap ;wd :action ActivateDebugToolWindow<cr>
nnoremap ;wr :action ActivateRunToolWindow<cr>
nnoremap ;wt :action ActivateTerminalToolWindow<cr>


" language
nnoremap gi :action GotoImplementation<cr>
nnoremap gt :action GotoTypeDeclaration<cr>
nnoremap gr :action ShowUsages<cr> | "go to reference
nnoremap gd :action QuickImplementations<cr>
nnoremap <leader>gd :action GotoDeclaration<cr>
nnoremap <leader>T :action GotoTest<cr>


" edit
nnoremap <leader>re :action RenameElement<cr>
nnoremap <leader>rf :action RenameFile<cr>

" debug
nnoremap <leader>dd :action Debug<cr>
nnoremap <leader>dc :action ChooseDebugConfiguration<cr>
nnoremap <leader>rr :action Run<cr>
nnoremap <leader>rc :action ChooseRunConfiguration<cr>

nnoremap <leader>gr :action GotoRelated<cr>
nnoremap <leader>gb :action Annotate<cr>

" use intellij back/forward instead
nnoremap <C-o> :action Back<cr>
nnoremap <C-i> :action Forward<cr>

nnoremap <C-h> <C-w>h
nnoremap <C-l> <C-w>l
nnoremap <c-k> <C-w>k
nnoremap <C-j> <C-w>j
nnoremap <c-c> :action ResizeToolWindowLeft<cr>

nnoremap ]t :action PreviousTab<cr>
nnoremap [t :action NextTab<cr>


" nnoremap <leader>q  :action CloseActiveTab<cr>
" nnoremap <leader>ga :action GotoAction<cr>
" nnoremap <leader>gs :action GotoSuperMethod<cr>

" breakpoint
nnoremap <leader>bb :action ToggleLineBreakpoint<cr>
nnoremap <leader>be :action ToggleBreakpointEnabled<cr>
nnoremap <leader>br :action ViewBreakpoints<cr>

nnoremap <leader>w :action AceWordAction<cr>

nnoremap <c-,> :action ShowSettings<cr>
set ideajoin

